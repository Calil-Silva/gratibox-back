{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "radix": 0,
        "prefer-destructuring": 0,
        "import/prefer-default-export": 0,
        "no-useless-escape": 0,
        "comma-dangle": 0
    }
}
